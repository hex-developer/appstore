{
  "$schema": "../app-config-schema.json",
  "name": "Immich",
  "available": true,
  "exposable": true,
  "port": 8128,
  "id": "immich",
  "tipi_version": 1,
  "version": "1.105.1",
  "categories": ["data", "photography"],
  "description": "Photo and video backup solution directly from your mobile phone.",
  "short_desc": "Self-hosted photo and video management solution.",
  "author": "Alex Tran",
  "website": "https://www.immich.app/",
  "source": "https://github.com/immich-app/immich",
  "dynamic_config": true,
  "form_fields": [
    {
      "type": "random",
      "label": "DB_PASSWORD",
      "min": 32,
      "env_variable": "DB_PASSWORD"
    },
    {
      "type": "random",
      "label": "JWT_SECRET",
      "min": 32,
      "env_variable": "JWT_SECRET"
    },
    {
      "type": "random",
      "label": "IMMICH_TYPESENSE_API_KEY",
      "min": 32,
      "env_variable": "IMMICH_TYPESENSE_API_KEY"
    }
  ],
  "supported_architectures": ["arm64", "amd64"]
}
